      <mass value="0.348"/>
      <inertia ixx="2.616060e-04" ixy="6.396800e-05" ixz="-5.560000e-07" iyy="4.185360e-04" iyz="-3.720000e-07" izz="5.112380e-04"/>
    </inertial>
