    </inertial>
  </link>

  <!-- joint_4 -->
  <joint name="joint_4" type="revolute">
    <origin xyz="${joint_4_xyz}" rpy="${joint_4_rpy}"/>
    <parent link="link_3"/>
    <child link="link_4"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit velocity="4.8" effort="300" lower="${-pi}" upper="${pi}" />
  </joint>

   <!-- link_4 -->
  <link name="link_4">
    <visual>
      <origin xyz="${link_4_xyz}" rpy="${link_4_rpy}"/>
      <geometry>
