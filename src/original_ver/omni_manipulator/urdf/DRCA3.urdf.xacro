      <inertia ixx="5.132895e-03" ixy="4.284000e-06" ixz="3.040000e-07" iyy="1.584478e-03" iyz="1.526370e-04" izz="4.085190e-03"/>
    </inertial>
  </link>

  <!-- joint_3 -->
  <joint name="joint_3" type="revolute">
    <origin xyz="${joint_3_xyz}" rpy="${joint_3_rpy}"/>
